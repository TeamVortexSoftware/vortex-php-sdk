{
    "name": "teamvortexsoftware/vortex-php-sdk",
    "description": "Vortex PHP SDK for authentication and invitation management",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Team Vortex Software",
            "email": "support@vortexsoftware.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "TeamVortexSoftware\\VortexSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TeamVortexSoftware\\VortexSDK\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
